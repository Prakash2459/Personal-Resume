{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out node_modules/.cache",
    "docker:build": "docker build -t resume-app .",
    "docker:run": "docker run -p 3000:3000 resume-app",
    "docker:dev": "docker build -f Dockerfile.dev -t resume-app-dev . && docker run -p 3000:3000 -v $(pwd):/app resume-app-dev"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "autoprefixer": "^10.4.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}
